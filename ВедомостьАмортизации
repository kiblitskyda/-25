		ПО (ИСТИНА)
			И (ВТ_НУ_СчетУчетаКонец.ОсновноеСредство = ВТ_НУ_ДанныеНУ.ОсновноеСредство)
			И (ВТ_НУ_СчетУчетаКонец.ВидУчета = ВТ_НУ_ДанныеНУ.ВидУчета)
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_НУ_СчетУчетаНачало КАК ВТ_НУ_СчетУчетаНачало
		ПО (ИСТИНА)
			И (ВТ_НУ_СчетУчетаНачало.ОсновноеСредство = ВТ_НУ_ДанныеНУ.ОсновноеСредство)
			И (ВТ_НУ_СчетУчетаНачало.ВидУчета = ВТ_НУ_ДанныеНУ.ВидУчета)
