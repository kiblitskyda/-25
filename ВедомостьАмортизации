		ПО (ИСТИНА)
			И (ВТ_НУ_СчетАмортизацииКонец.ОсновноеСредство = ВТ_НУ_ДанныеНУ.ОсновноеСредство)
			И (ВТ_НУ_СчетАмортизацииКонец.ВидУчета = ВТ_НУ_ДанныеНУ.ВидУчета)
